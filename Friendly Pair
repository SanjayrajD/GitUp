#include<iostream>
using namespace std;
int main()
{
    int n,m,i,a,b;
    cin>>n>>m;
    int sum1=0,sum2=0;
    for(i=1;i<n;i++)
    {
        if(n%i==0)
        {
            sum1+=i;
        }
        if(m%i==0)
        {
            sum2+=i;
        }
    }
    a=sum1/n;
    b=sum2/n;
    if(a==b)
    {
        cout<<"Friendly pair";
    }
    else
    {
        cout<<"Not Friendly Pair";
    }
}
